{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins"
    ]
  },
  "User": {
    "dataSource": "logbookDB",
    "public": false
  },
  "AccessToken": {
    "dataSource": "logbookDB",
    "public": false
  },
  "ACL": {
    "dataSource": "logbookDB",
    "public": false
  },
  "RoleMapping": {
    "dataSource": "logbookDB",
    "public": true,
    "options": {
      "strictObjectIDCoercion": true
    }
  },
  "Role": {
    "dataSource": "logbookDB",
    "public": true
  },
  "Company": {
    "dataSource": "logbookDB",
    "public": true
  },
  "Meeting": {
    "dataSource": "logbookDB",
    "public": true
  },
  "Leave": {
    "dataSource": "logbookDB",
    "public": true
  },
  "Objective": {
    "dataSource": "logbookDB",
    "public": true
  },
     "Email": {
    "dataSource": "emailDs"
  },
  "Activity": {
    "dataSource": "logbookDB",
    "public": true
  },
  "logUser": {
    "dataSource": "logbookDB",
    "public": true,
     "options": { "emailVerificationRequired": false}
  }
}
